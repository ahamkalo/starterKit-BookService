<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-preview-i18n-resource ../bundle/bundle.properties?>
<?scenebuilder-stylesheet ../css/standard.css?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>

<AnchorPane prefHeight="180.0" prefWidth="500" xmlns="http://javafx.com/javafx/8.0.51" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.capgemini.starterkit.bookService.controller.AddAuthorController">
  <Label layoutX="170.0" layoutY="14.0" styleClass="formTitle" text="%label.addAuthor" />
  <GridPane hgap="10.0" layoutX="14.0" layoutY="60.0" vgap="10.0">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="90.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="230.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="120.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
      <Label text="%label.firstName" />
      <TextField fx:id="firstNameField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
      <Label text="%label.lastName" GridPane.rowIndex="1" />
      <TextField fx:id="lastNameField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="addAuthorButton" defaultButton="true" minWidth="10.0" mnemonicParsing="false" onAction="#addAuthorButtonAction" prefWidth="120.0" text="%button.addAuthor" GridPane.columnIndex="2" />
    </children>
  </GridPane>
</AnchorPane>
